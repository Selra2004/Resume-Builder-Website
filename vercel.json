{
  "version": 2,
  "framework": "vite",
  "buildCommand": "npm run vercel-install && npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm run vercel-install",
  "devCommand": "npm run dev",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
