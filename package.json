{
  "name": "acc4",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": "22.20.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "server": "tsx watch server/index.ts",
    "ai-service": "cd recommendation_service && python main.py",
    "ai-service:install": "cd recommendation_service && pip install -r requirements-minimal.txt && pip install -r requirements-ml.txt",
    "start": "concurrently \"npm run dev\" \"npm run server\" \"npm run ai-service\"",
    "start:win": "concurrently \"npm run dev\" \"set JWT_SECRET=acc_portal_secret_2024 && npm run server\" \"npm run ai-service\"",
    "dev:full": "concurrently --names \"Frontend,Backend,AI-Service\" --prefix-colors \"cyan,green,magenta\" \"npm run dev\" \"npm run server\" \"npm run ai-service\""
  },
  "dependencies": {
